<!--===HEADER SECTION===-->
<table align="center" width="<%= current_page.data.table_width %>" id="headerSection" class="fixed-table" cellpadding="0" cellspacing="0" border="0">
  <tr>
    <td align="center" valign="top" <%= current_page.data.headerOuterBG %>>
      <table align="center" width="<%= current_page.data.nested_width %>" class="fixed-table" cellpadding="0" cellspacing="0" border="0" <%= current_page.data.headerBG %>>
        <tr>
          <td valign="top" align="center"><a alias="hdrImg" href="%%=RedirectTo(@promoURL)=%%" target="_blank" style="text-decoration: none; <%= current_page.data.headerColor %>"><img id="hdr-img" src="<%= current_page.data.hdrimg %>" width="<%= current_page.data.hdr_width %>" border="0" alt="<%= current_page.data.hdr_alt %>" style="display:block;height:auto; font-family:Arial, Helvetica, sans-serif; font-size:20px; <%= current_page.data.headerColor %> line-height:25px; text-decoration: none;" class="full-img" /></a></td>
        </tr>
      </table>
    </td>
  </tr>
</table>
<!--===END HEADER SECTION===-->