<!--===HEADER SECTION===-->
<table align="center" width="<%= current_page.data.table_table_width %>" class="fixed-table" cellpadding="0" cellspacing="0" border="0" style="width: <%= current_page.data.css_table_width %>;">
  <tr>
    <td align="center" valign="top" <%= current_page.data.headerOuterBG %>>
      <table align="center" width="<%= current_page.data.table_nested_width %>" class="fixed-table" cellpadding="0" cellspacing="0" border="0" <%= current_page.data.headerBG %> style="width: <%= current_page.data.css_nested_width %>;">
        <tr>
          <td valign="top" align="center"><a alias="hdrImg" href="%%=RedirectTo(@promoURL)=%%" target="_blank" style="text-decoration: none; <%= current_page.data.headerColor %>"><img id="hdr-img" src="<%= current_page.data.hdrimg %>" width="<%= current_page.data.table_hdr_width %>" border="0" alt="<%= current_page.data.hdr_alt %>" style="display:block; width:<%= current_page.data.css_hdr_width %>; height:auto; font-family: Arial, Helvetica, sans-serif; font-size:20px; <%= current_page.data.headerColor %> line-height:25px; text-decoration: none;" class="full-img" /></a></td>
        </tr>
      </table>
    </td>
  </tr>
</table>
<!--===END HEADER SECTION===-->