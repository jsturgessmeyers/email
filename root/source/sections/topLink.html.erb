<!--===PREHEADER===-->
<table align="center" width="<%= current_page.data.table_width %>" class="fixed-table" cellpadding="0" cellspacing="0" border="0">
  <tr>
    <td align="center" valign="top" <%= current_page.data.topLinksOuterBG %>>
      <table <%= current_page.data.topLinksBG %> align="center" width="<%= current_page.data.nested_width %>" class="fixed-table" cellpadding="0" cellspacing="0" border="0">
        <tr>
          <td align="center" style="font-family:Arial, Helvetica, sans-serif; font-size:10px; <%= current_page.data.topLinksColor %> -webkit-text-size-adjust:none; padding-top:7px; padding-bottom:7px;"><a alias="topLink" href="%%=RedirectTo(@promoURL)=%%" target="_blank" style="text-decoration:none;"><span style="<%= current_page.data.topLinksColor %> text-decoration:underline;"><%= current_page.data.topLink %></span></a> <span class="hide-mobile">| <a alias="viewInBrowser" href="%%view_email_url%%" target="_blank" style="text-decoration:none;"><span style="<%= current_page.data.topLinksColor %> text-decoration:underline;">View in your browser</span></a></span></td>
        </tr>
      </table>
    </td>
  </tr>
</table>
