<!--===FOOTER===-->
<table align="center" width="<%= current_page.data.table_width %>" id="footer-table" class="fixed-table" cellpadding="0" cellspacing="0" border="0">
  <tr>
    <td class="remove-pad" align="center" valign="top">
      <table align="center" width="<%= current_page.data.nested_width %>" class="fixed-table" border="0" cellpadding="0" cellspacing="0">
        <tr>
          <td class="remove-pad" valign="top" style="padding-bottom:20px;">
            <table align="center" <%= current_page.data.ftr_bg_color %> width="100%" cellpadding="0" cellspacing="0" border="0">
              <tr>
                <td name="disclosure" align="center" valign="top" <%= current_page.data.ftr_bg_color %> style="<%= current_page.data.ftr_color %> font-family:Arial, Helvetica, sans-serif; font-size:10px; line-height:14px; -webkit-text-size-adjust:none; padding-top:10px; padding-right:10px; padding-bottom:10px; padding-left:10px;">*Offer expires %%=Format(DateAdd(Now(), "<%= current_page.data.expDays %>","D"), "MM/dd/yyyy")=%%. Certain restrictions apply. <a alias="ftrpromo" href="%%=RedirectTo(@promoURL)=%%" target="_blank" style="text-decoration: none;"><span style="text-decoration: underline; <%= current_page.data.ftr_color %>">Go here</span></a> or <a alias="ftrCSPHONE" href="tel:%%=v(@CSPHONE)=%%" target="_blank" style="text-decoration: none;"><span style="<%= current_page.data.ftr_color %>">call</span></a> for more details.
                </td>
              </tr>
              <tr>
                <td name="noreply" align="center" valign="top" <%= current_page.data.ftr_bg_color %> style="<%= current_page.data.ftr_color %> font-family:Arial, Helvetica, sans-serif; font-size:10px; line-height:14px; -webkit-text-size-adjust:none; padding-top:10px; padding-right:10px; padding-bottom:10px; padding-left:10px;">This message has been sent from a mailbox that cannot receive replies. We're happy to help you with any questions or concerns you have regarding your account or a promotional offer. Please contact us directly at <a alias="noreplyCSEMAIL" name="noreply" href="mailto:%%=v(@CSEMAIL)=%%" target="_blank" style="text-decoration: none;"><span style="text-decoration: underline; <%= current_page.data.ftr_color %> white-space: nowrap;">%%=v(@CSEMAIL)=%%</span></a> or call <a alias="noreplyCSPHONE" href="tel:%%=v(@CSPHONE)=%%" target="_blank" style="text-decoration: none;"><span style=" <%= current_page.data.ftr_color %> white-space: nowrap;">%%=v(@CSPHONE)=%%</span></a>.
                </td>
              </tr>
              <tr>
                <td name="unsub" align="center" valign="top" <%= current_page.data.ftr_bg_color %> style="<%= current_page.data.ftr_color %> font-family:Arial, Helvetica, sans-serif; font-size:10px; line-height:14px; -webkit-text-size-adjust:none; padding-top:10px; padding-right:10px; padding-bottom:10px; padding-left:10px;">This email was sent to: <a href="#" style="<%= current_page.data.ftr_color %> text-decoration:none; font-weight:bold;">%%emailaddr%%.</a>
                <br/>To unsubscribe from future circulation promotions from %%=Lookup("ATVAL","PUBNAME_THE","PubBrand",@myPubBrand)=%%,
                <br /><a alias="unsubscribe" name="unsub" href="%%=RedirectTo(@unsubscribeURL)=%%" title="unsubscribe from future offers" style="<%= current_page.data.ftr_color %>">visit the following web page</a>.
                </td>
              </tr>
              <tr>
                <td name="address" align="center" valign="top" <%= current_page.data.ftr_bg_color %> style="<%= current_page.data.ftr_color %> font-family:Arial, Helvetica, sans-serif; font-size:10px; line-height:14px; -webkit-text-size-adjust:none; padding-top:10px; padding-right:10px; padding-bottom:10px; padding-left:10px;">%%=Lookup("ATVAL","PUBADDRESS","PubBrand",@myPubBrand)=%% | %%=Lookup("ATVAL","PUBPCITY","PubBrand",@myPubBrand)=%%, %%=Lookup("ATVAL","PUBSTATE","PubBrand",@myPubBrand)=%% | %%=Lookup("ATVAL","PUBPZIP","PubBrand",@myPubBrand)=%% | %%Member_Country%%<br/>&copy; <b>%%=Lookup("ATVAL","PUBNAME","PubBrand",@myPubBrand)=%%</b>
                </td>
              </tr>
            </table>
          </td>
        </tr>
      </table>
    </td>
  </tr>
</table>
<!--===END FOOTER===-->