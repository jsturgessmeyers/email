<!--===FOOTER===-->
<table align="center" width="<%= current_page.data.table_table_width %>" class="fixed-table" border="0" cellpadding="0" cellspacing="0" style="width: <%= current_page.data.css_table_width %>;">
  <tr>
    <td align="center" valign="top" <%= current_page.data.footerOuterBG %>>
      <table align="center" <%= current_page.data.footerBG %> width="<%= current_page.data.table_nested_width %>" class="fixed-table" cellpadding="0" cellspacing="0" border="0" style="width: <%= current_page.data.css_nested_width %>;">
        <tr>
          <td name="disclosure" align="center" valign="top" <%= current_page.data.ftr_bg_color %> style="<%= current_page.data.footerColor %> font-family:Arial, Helvetica, sans-serif; font-size:10px; line-height:14px; -webkit-text-size-adjust:none; padding-top:20px; padding-right:20px; padding-bottom:10px; padding-left:20px;">*Offer expires %%=Format(DateAdd(Now(), "<%= current_page.data.expDays %>","D"), "MM/dd/yyyy")=%%. Certain restrictions apply. <a alias="ftrpromo" href="%%=RedirectTo(@promoURL)=%%" target="_blank" style="text-decoration: none;"><span style="text-decoration: underline; <%= current_page.data.footerColor %>">Go here</span></a> or <a alias="ftrCSPHONE" href="tel:%%=v(@CSPHONE)=%%" target="_blank" style="text-decoration: none;"><span style="<%= current_page.data.footerColor %>">call</span></a> for more details.</td>
        </tr>
        <tr>
          <td name="noreply" align="center" valign="top" <%= current_page.data.ftr_bg_color %> style="<%= current_page.data.footerColor %> font-family:Arial, Helvetica, sans-serif; font-size:10px; line-height:14px; -webkit-text-size-adjust:none; padding-top:10px; padding-right:20px; padding-bottom:10px; padding-left:20px;">This message has been sent from a mailbox that cannot receive replies. We're happy to help you with any questions or concerns you have regarding your account or a promotional offer. Please contact us directly at <a alias="noreplyCSEMAIL" name="noreply" href="mailto:%%=v(@CSEMAIL)=%%" target="_blank" style="text-decoration: none;"><span style="text-decoration: underline; <%= current_page.data.footerColor %> white-space: nowrap;">%%=v(@CSEMAIL)=%%</span></a> or call <a alias="noreplyCSPHONE" href="tel:%%=v(@CSPHONE)=%%" target="_blank" style="text-decoration: none;"><span style=" <%= current_page.data.footerColor %> white-space: nowrap;">%%=v(@CSPHONE)=%%</span></a>.</td>
        </tr>
        <tr>
          <td name="unsub" align="center" valign="top" <%= current_page.data.ftr_bg_color %> style="<%= current_page.data.footerColor %> font-family:Arial, Helvetica, sans-serif; font-size:10px; line-height:14px; -webkit-text-size-adjust:none; padding-top:10px; padding-right:20px; padding-bottom:10px; padding-left:20px;">This email was sent to: <a href="#" style="<%= current_page.data.footerColor %> text-decoration:none; font-weight:bold;">%%emailaddr%%.</a><br/>To unsubscribe from future circulation promotions from %%=Lookup("ent.ATVAL","PUBNAME_THE","PubBrand",@myPubBrand)=%%,<br /><a alias="unsubscribe" name="unsub" href="%%=RedirectTo(@unsubscribeURL)=%%" title="unsubscribe from future offers" style="<%= current_page.data.footerColor %>">visit the following web page</a>.</td>
        </tr>
        <tr>
          <td name="address" align="center" valign="top" <%= current_page.data.ftr_bg_color %> style="<%= current_page.data.footerColor %> font-family:Arial, Helvetica, sans-serif; font-size:10px; line-height:14px; -webkit-text-size-adjust:none; padding-top:10px; padding-right:20px; padding-bottom:20px; padding-left:20px;">%%=Lookup("ent.ATVAL","PUBADDRESS","PubBrand",@myPubBrand)=%% | %%=Lookup("ent.ATVAL","PUBPCITY","PubBrand",@myPubBrand)=%%, %%=Lookup("ent.ATVAL","PUBSTATE","PubBrand",@myPubBrand)=%% | %%=Lookup("ent.ATVAL","PUBPZIP","PubBrand",@myPubBrand)=%% | %%Member_Country%%<br/>&copy; <b>%%=Lookup("ent.ATVAL","PUBNAME","PubBrand",@myPubBrand)=%%</b></td>
        </tr>
      </table>
    </td>
  </tr>
</table>
<!--===END FOOTER===-->